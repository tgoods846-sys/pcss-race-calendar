{
    "outputDirectory": "site",
    "headers": [
        {
            "source": "/pcss-calendar.ics",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/calendar; charset=utf-8"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOWALL"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors *"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}
